{
  "name": "peddle-api",
  "version": "1.0.0",
  "description": "Peddle backend service",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "npm run build && node ./build/app.js",
    "dev": "nodemon ./src/app.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run lint && npm run build:types && npm run build:js",
    "build:types": "tsc  --emitDeclarationOnly --declarationDir build",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "keywords": [],
  "author": "tylerdgenius",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.16",
    "@types/morgan": "^1.9.4",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "jest": "^29.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.0",
    "morgan": "^1.10.0",
    "normal-case-generator": "^1.0.1",
    "socket.io": "^4.5.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
